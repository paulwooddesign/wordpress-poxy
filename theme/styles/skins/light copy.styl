///////////////////////////////////////////////
// Main Settings
///////////////////////////////////////////////
::selection
  background accent
  color black

html
  background-color $main_background_color

body
  opacity 1
  color font_color
  background-color $main_background_color

.header
  background header_background_color

#container
  background-color container_background_color

footer
  background-size initial
  // background:#e9e3dd;

a
