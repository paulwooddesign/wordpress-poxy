///////////////////////////////////////////////
// SVG Settings
///////////////////////////////////////////////
nav
  svg:hover path
    fill rgba(accent, 1)

  path
    fill font_color

  .accent
    svg:hover path
      fill font_color

    path
      fill rgba(accent, 1)

  .white
    svg:hover path
      fill rgba(white, 1)

    path
      fill rgba(accent, 1)


