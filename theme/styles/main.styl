/////////////////////////////////
// Thumbs
/////////////////////////////////
$trans_duration = 150ms




.thumbs
  figure
    transition $trans_duration ease
    //box-shadow: inset 0 .2em .5em 0 #FFF, inset 0 -0.3em .4em 0px rgba(0, 0, 0, .5), 0 1.5em 3em -1em rgba(0, 0, 0, 0.5);
    //box-shadow inset 0 .2em .5em 0 #FFF, inset 0 -0.3em .4em 0px rgba(0, 0, 0, .2), 0 1.5em 3em -1em rgba(0, 0, 0, 0.5)
    position relative
    background-image linear-gradient(to bottom, #111, #333)

    // box-shadow 0 .5em 2em rgba(0,0,0,.05), inset 0 .2em .4em -.2em rgba(255,255,255,.2), 0 0 .2em rgba(0,0,0,.1)
    // box-shadow 0 .5em 2em rgba(255,255,255,.02), inset 0 .2em .4em -.2em rgba(0,0,0,.2), 0 0 .2em rgba(0,0,0,.2)

    background linear-gradient( to bottom, skin_highlight_color, skin_shadow_color )
    .thumb-title
      color #222
      opacity 0

    .thumb
      // width 94%
      // height 94%
      width 97%
      height 97%
      transition $trans_duration ease
      // box-shadow inset 0 .2em .5em 0 #FFF, inset 0 -0.3em .4em 0px rgba(0, 0, 0, .2)
      // background-color rgba(0,0,0,.7)
      // background-position 50% 0%
      // filter: grayscale(70%)

      // background-image linear-gradient(-45deg, rgba(#000,1) 0%,  rgba(#444,1) 100%)
      background-image linear-gradient( to bottom, skin_shadow_color, skin_highlight_color)

    .icon
      opacity .8

    .shadow
      // width 94%
      // height 94%
      width 97%
      height 97%
      transition opacity $trans_duration ease
      box-shadow none
      opacity .6
      background-image radial-gradient(ellipse at center, rgba(0,0,0, .1), rgba(0,0,0,0.6) 50%, rgba(0,0,0,0.95) 110%)
      text-align center


    .fill-94
      width 97%
      height 97%


    .gold-filter
      transition transform $trans_duration
      // width 95%
      // height 95%
      width 98%
      height 98%
      opacity .7
      background-image linear-gradient(45deg, rgba(#463c2c,1) 0%, rgba(#fdd333,1) 26%, rgba(#fffcaf,1) 35%, rgba(#b67b03,1) 55%, rgba(#341c02,1) 80%, rgba(#160f07,1) 100%)

    .accent-border
      opacity 0

    .highlight-border
      opacity 0
      background-image linear-gradient(0deg, rgba(#fff,.0) 0%,  rgba(#fff,.3) 100%)

    .fill-95
      transition $trans_duration ease
      // width 95%
      // height 95%
      width 98%
      height 98%

    .title
      opacity 0
      transition $trans_duration ease
      transform translateX(50%) translateY(100%)


  figure:hover
    .gloss
      border-radius 20%
      transition $trans_duration
      //box-shadow inset 5em 5em 0 -16em rgba(255,255,255,0.3)
      box-shadow inset 16em 18em 0 -13em rgba(255,255,255,0.1)
    .thumb
      // box-shadow inset 0 .2em .5em 0 #FFF, inset 0 -0.3em .4em 0px rgba(0, 0, 0, .2)
      filter: grayscale(0%)
      width 97%
      height 97%
      width 94%
      height 94%

    .shadow
      transition $trans_duration ease
      opacity .8
      width 97%
      height 97%
      width 94%
      height 94%

    .gold-filter
      transition opacity $trans_duration
      opacity 1

    .color-filter
      opacity .0
      transition transform $trans_duration
      //transform translateX(50%) translateY(-100%)
      //background-image linear-gradient(45deg, rgba(#463c2c,.3) 0%, rgba(#fdd333,.3) 26%, rgba(#fffcaf,.5) 65%, rgba(#b67b03,.4) 55%, rgba(#341c02,.3) 80%, rgba(#160f07,.3) 100%)

    .fill-94
      width 94%
      height 94%

    .fill-95
      transition $trans_duration ease
      width 98%
      height 98%
      width 95%
      height 95%

    .accent-border,
    .highlight-border
      opacity 1

    .title
      opacity 1
      transition $trans_duration ease
      transform translateX(50%) translateY(0%)







//////////////////////////////////////////////////////////////////////
// Thumb hover Slide
/////////////////////////////////////////////////////////////////////
.bg__slide
  // background-color red
  // background-image url("https://dl.dropboxusercontent.com/s/c8vk1vwcluzcsk8/simple-slideshow-slides.jpg?dl=0") !important
  background-repeat no-repeat
  background-position center left
  // filter: grayscale(50%)


  opacity 0


a:hover .bg__slide
  opacity 1
  /* ANIMATING STUFF */
  animation-name slide
  animation-duration 6s
  animation-direction normal
  animation-timing-function ease
  background-position center left
  // animation-iteration-count infinite


@keyframes slide {
  0% {background-position:0 0;}
  20% {background-position:0% 0;}
  25% {background-position:25% 0;}
  50% {background-position:25% 0;}
  55% {background-position:50% 0;}
  70% {background-position:50% 0;}
  75% {background-position:75% 0;}
  95% {background-position:75% 0;}
  100% {background-position:100% 0;}
}


