.op0,
.wp1,
.wp2,
.wp3,
.wp4
  opacity 0

.fadeIn .wp1
  opacity 1 !important
  transition opacity 0.2s
  animation animation 1500ms linear both

.fadeIn .wp2
  opacity 1 !important
  transition opacity 0.2s
  transition-delay 200ms
  animation animation_d1 1700ms linear both

.fadeIn .wp3
  opacity 1 !important
  transition opacity 0.2s
  transition-delay 400ms
  animation animation_d2 1900ms linear both

.fadeIn .wp4
  opacity 1 !important
  transition opacity 0.2s
  transition-delay 600ms
  animation animation_d3 2100ms linear both




.fade-out
  // transform translate(0%, 20%)
  opacity 0 !important
  transition 0.3s

.fade-in
  // transform translate(0%, 0%)
  opacity 1 !important
  transition 0.3s


.fadeOut
  opacity 0 !important
  transition opacity 0.2s

.fadeIn
  opacity 1 !important
  transition opacity 0.2s



.waterfall > .fadeIn:nth-child(4n+1)
  animation animation 1500ms linear both
.waterfall > .fadeIn:nth-child(4n+2)
  transition-delay 200ms
  animation animation_d1 1700ms linear both
.waterfall > .fadeIn:nth-child(4n+3)
  transition-delay 400ms
  animation animation_d2 1900ms linear both
.waterfall > .fadeIn:nth-child(4n+4)
  transition-delay 600ms
  animation animation_d3 2100ms linear both

